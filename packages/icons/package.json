{
  "name": "pcln-icons",
  "version": "1.0.0-0",
  "description": "Priceline React icons based on Material Design Icons",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "babel src -d lib",
    "clean": "rm -rf svg/material-design src/material-design src/custom lib",
    "build": "npm run clean && ./build.js && pixo svg/material-design -d src/material-design && pixo svg/custom -d src/custom",
    "test": "jest"
  },
  "author": "Priceline",
  "license": "MIT",
  "dependencies": {
    "lodash.upperfirst": "^4.3.1",
    "styled-system": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.uniqby": "^4.7.0",
    "material-design-icons": "^3.0.1",
    "pcln-design-system": "^1.0.0-70",
    "pixo": "^1.1.1-0",
    "react": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "styled-components": "^3.4.5"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ]
  },
  "pixo": {
    "template": "styledSystem",
    "outDir": "src/icons",
    "index": true
  }
}
