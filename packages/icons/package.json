{
  "name": "pcln-icons",
  "version": "1.0.0",
  "description": "Priceline React icons based on Material Design Icons",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "babel src -d lib",
    "clean": "rm -rf svg src lib",
    "build": "npm run clean && ./build.js && pixo svg --template styledSystem -icd src",
    "test": "jest"
  },
  "author": "Priceline",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^23.5.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.upperfirst": "^4.3.1",
    "material-design-icons": "^3.0.1",
    "pixo": "^1.1.0",
    "react": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "styled-components": "^3.4.5"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ]
  }
}
